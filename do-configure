cmake -S. -B"/home/michaelbishop/softwares/psi4_mod/psi4-1.3.x/compile-psi4" \
        -DCMAKE_INSTALL_PREFIX="/home/michaelbishop/softwares/psi4_mod/psi4-1.3.x/install-psi4" \
        -DCMAKE_PREFIX_PATH="/home/michaelbishop/softwares/psi4_mod/psi4-1.3.x/install-psi4/externals/install-libint" \
        -DMAX_AM_ERI=6 \
        -DENABLE_gdma=ON \
        -DBUILD_SHARED_LIBS=ON \
        -DCMAKE_C_COMPILER=/usr/bin/gcc \
        -DCMAKE_CXX_COMPILER=/usr/bin/g++ \
        -DCMAKE_Fortran_COMPILER=/usr/bin/gfortran \
        -DBLAS_TYPE=MKL \
        -DLAPACK_TYPE=MKL \
        -DBLAS_LIBRARIES=/home/michaelbishop/softwares/miniconda3/envs/psi4mod/lib/libmkl_rt.so \
        -DLAPACK_LIBRARIES=/home/michaelbishop/softwares/miniconda3/envs/psi4mod/lib/libmkl_rt.so \
        -DBLAS_INCLUDE_DIRS=/home/michaelbishop/softwares/miniconda3/envs/psi4mod/include/ \
        -DLAPACK_INCLUDE_DIRS=/home/michaelbishop/softwares/miniconda3/envs/psi4mod/include/ \
        -DOpenMP_LIBRARY_DIRS=/home/michaelbishop/softwares/miniconda3/envs/psi4mod/lib/
